#include <stdio.h>
int solve()
{
    int n, i, c1=0, c2=0, m1=0, m2=0;
    scanf("%d", &n);
    int a[n], b[n];
    for (i=0; i<n; i++)
    {
        scanf("%d", &a[i]);
    }
    for (i=0; i<n; i++)
    {
        scanf("%d", &b[i]);
    }
    for(i=0; i<n; i++)
    {
        if(a[i]>0)
        {
            c1++;
            if(c1>m1)
            {m1=c1;}
        }
        else 
        {
            c1=0;
        }
        if (b[i]>0)
        {
            c2++;
            if(c2>m2)
            {m2=c2;}
        }
        else 
        {
            c2=0;
        }
    }
    if (m1>m2)
    {
        printf("Om\n");
    }
    else if(m2>m1)
    {
        printf("Addy\n");
    }
    else
    {
        printf("Draw\n");
    }
}
int main() {
    int t;
    scanf("%d", &t);
    while(t--)
    {
        solve();
    }
	return 0;
}
